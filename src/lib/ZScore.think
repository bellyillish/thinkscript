###
# ZScore
#   - data
#   - window
###

script ZScore {
    input data   = 0;
    input window = 90;

    def mean = Average(data, window);
    def dev  = StDev(data, window);

    plot zscore;
    if dev != 0 {
        zscore = (data - mean) / dev;
    }
    else {
        zscore = Double.NaN;
    }
}
