script ZScore {
    input value  = 0;
    input window = 90;

    def mean = Average(value, window);
    def dev  = StDev(value, window);

    def zscore;
    if dev != 0 {
        zscore = (value - mean) / dev;
    }
    else {
        zscore = Double.NaN;
    }

    plot P = zscore;
    P.Hide();
}
